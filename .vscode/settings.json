// Place your settings in this file to overwrite default and user settings.
{
    "minify.css": {
        "advanced": true,
        "aggressiveMerging": true,
        "compatibility": "*",
        "inliner": {},
        "keepBreaks": false,
        "keepSpecialComments": "*",
        "mediaMerging": true,
        "processImport": true,
        "processImportFrom": [
            "all"
        ],
        "rebase": true,
        "restructuring": true,
        "root": "${/assets/scripts/ui.js}",
        "roundingPrecision": 2,
        "semanticMerging": false,
        "shorthandCompacting": true
    },
    "minify.minifyExistingOnSave": true
}